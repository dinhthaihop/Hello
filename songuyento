void quesion3()
{
    int i;
    printf("so nguyen to tu 1 den 100 la: ");
    for(i=1;i<=100;i++)
    {
        if(songuyento(i))
        printf("\t%d", i);
    }
}
int songuyento(int n)
{
    int tam=1,i;
    for(i=2;i<n;i++)
    {
        if(n%i==0)
        {
            tam=0;
            break;
        }
    }
    return tam;
